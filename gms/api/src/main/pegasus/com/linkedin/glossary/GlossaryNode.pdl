namespace com.linkedin.glossary

import com.linkedin.common.GlossaryNodeUrn
import com.linkedin.common.Ownership

/**
 * Metadata for a business node
 */
record GlossaryNode {

  /**
   * Business node urn
   */
  urn: GlossaryNodeUrn

  /**
   * Business node info
   */
  glossaryNodeInfo: optional GlossaryNodeInfo

  /**
   * Ownership
   */
  ownership: optional Ownership
}