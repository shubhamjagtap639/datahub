namespace com.linkedin.glossary

import com.linkedin.common.GlossaryTermUrn
import com.linkedin.common.Ownership

/**
 * Metadata for a business term
 */
record GlossaryTerm {

  /**
   * Business Term urn
   */
  urn: GlossaryTermUrn

  /**
   * Business Term Info
   */
  glossaryTermInfo: optional GlossaryTermInfo

  /**
   * Ownership
   */
  ownership: optional Ownership
}