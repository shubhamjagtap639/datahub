{
  "index": {
    "max_ngram_diff": "47",
    "analysis": {
      "filter": {
        "autocomplete_filter": {
          "type": "edge_ngram",
          "min_gram": "3",
          "max_gram": "50"
        },
        "custom_delimiter": {
          "split_on_numerics": "false",
          "split_on_case_change": "false",
          "type": "word_delimiter",
          "preserve_original": "true",
          "catenate_words": "false"
        },
        "urn_stop_filter": {
          "type": "stop",
          "stopwords": [
            "urn",
            "li",
            "dataset",
            "dataplatform"
          ]
        }
      },
      "normalizer": {
        "my_normalizer": {
          "filter": [
            "lowercase"
          ],
          "type": "custom"
        }
      },
      "analyzer": {
        "whitespace_lowercase": {
          "filter": [
            "lowercase"
          ],
          "tokenizer": "whitespace"
        },
        "slash_pattern": {
          "filter": [
            "lowercase"
          ],
          "type": "custom",
          "tokenizer": "slash_tokenizer"
        },
        "ml_model_pattern": {
          "filter": [
            "lowercase"
          ],
          "type": "custom",
          "tokenizer": "ml_model_pattern"
        },
        "custom_browse": {
          "filter": [
            "lowercase"
          ],
          "type": "custom",
          "tokenizer": "path_hierarchy_tokenizer"
        },
        "custom_ngram": {
          "filter": [
            "lowercase"
          ],
          "type": "custom",
          "tokenizer": "custom_ngram"
        },
        "custom_keyword": {
          "filter": [
            "lowercase"
          ],
          "type": "custom",
          "tokenizer": "keyword"
        },
        "delimit": {
          "filter": [
            "lowercase",
            "custom_delimiter"
          ],
          "tokenizer": "whitespace"
        },
        "ml_model_pattern_ngram": {
          "filter": [
            "lowercase",
            "autocomplete_filter"
          ],
          "type": "custom",
          "tokenizer": "ml_model_pattern"
        },
        "custom_browse_slash": {
          "filter": [
            "lowercase"
          ],
          "type": "custom",
          "tokenizer": "path_hierarchy"
        },
        "urn_component": {
          "filter": [
            "urn_stop_filter"
          ],
          "type": "custom",
          "tokenizer": "urn_char_group"
        }
      },
      "tokenizer": {
        "path_hierarchy_tokenizer": {
          "type": "path_hierarchy",
          "replacement": "/",
          "delimiter": "."
        },
        "custom_ngram": {
          "type": "ngram",
          "min_gram": "3",
          "max_gram": "50"
        },
        "slash_tokenizer": {
          "pattern": "[/]",
          "type": "pattern"
        },
        "ml_model_pattern": {
          "pattern": "[./]",
          "type": "pattern"
        },
        "urn_char_group": {
          "type": "pattern",
          "pattern": "[:\\s(),]"
        }
      }
    }
  }
}
