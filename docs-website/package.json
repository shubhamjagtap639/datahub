{
  "name": "docs-website",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "serve": "docusaurus serve",
    "clear": "docusaurus clear && rm -rf genDocs/*",
    "generate": "rm -rf genDocs/* && ts-node generateDocsDir.ts",
    "lint": "prettier -w generateDocsDir.ts sidebars.js",
    "lint-check": "prettier -l generateDocsDir.ts sidebars.js"
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-alpha.69",
    "@docusaurus/preset-classic": "2.0.0-alpha.69",
    "@docusaurus/plugin-ideal-image": "2.0.0-alpha.69",
    "clsx": "^1.1.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  }
}
