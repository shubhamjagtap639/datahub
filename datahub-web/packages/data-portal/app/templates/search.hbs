<div class="row">
  <Search::SearchMain
    @searchConfig={{this.model.searchConfig}}
    @trackPrefix="searchResult"
    @shouldCollectSearchTelemetry={{true}}
    @keyword={{this.keyword}}
    @facets={{this.facets}}
    @page={{this.page}}
    @entity={{this.entity}}
    @fields={{this.model.fields}}
    @showFacets={{this.model.showFacets}}
    @onChangeEntity={{action this.onChangeEntity}}
    @useMultiEntity={{true}}
  />
</div>

