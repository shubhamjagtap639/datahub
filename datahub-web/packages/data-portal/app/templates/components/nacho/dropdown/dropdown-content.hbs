{{#component
  baseComponent
  onMouseEnter=onMouseEnter
  onMouseLeave=onMouseLeave
}}
  <ul class="nacho-drop-down__options">
    {{#each options as |option|}}
      {{component
        "nacho/dropdown/dropdown-option"
        option=option
        onSelect=onSelect
        selectedOption=selectedOption
      }}
    {{/each}}
  </ul>
{{/component}}
