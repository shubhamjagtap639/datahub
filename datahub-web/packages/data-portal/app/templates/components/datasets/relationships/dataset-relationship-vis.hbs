<div class="dataset-relashipship-vis__container"></div>
<div class="dataset-relationship-vis__tool-bar">
  <button class="nacho-button nacho-button--tertiary" {{action "zoomIn"}}>
    {{fa-icon "search-plus" aria-label="Zoom in"}}
  </button>
  <button class="nacho-button nacho-button--tertiary" {{action "zoomOut"}}>
    {{fa-icon "search-minus" aria-label="Zoom out"}}
  </button>
  <button class="nacho-button nacho-button--tertiary" {{action "fitGraph"}}>
    {{fa-icon "compress-arrows-alt" aria-label="Fit graph"}}
  </button>
</div>
<div class="dataset-relationship-vis__legend">
  <h6>Legend</h6>
  <div>
    <i
      class="dataset-relationship-vis__node dataset-relationship-vis__node--unselected"
    >
    </i>
    Unselected
  </div>
  <div>
    <i
      class="dataset-relationship-vis__node dataset-relationship-vis__node--unloaded"
    >
    </i>
    Parent/Child not loaded
  </div>
  <div>
    <i
      class="dataset-relationship-vis__node dataset-relationship-vis__node--selected"
    >
    </i>
    Selected Node
  </div>
</div>
