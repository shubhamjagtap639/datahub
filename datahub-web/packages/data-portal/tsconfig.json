{
  "extends": "../../configs/tsconfig-base",
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": "../../",
    "alwaysStrict": false,
    "strictPropertyInitialization": false,
    "lib": [
      "es2017.object",
      "es2016",
      "dom",
      "dom.iterable",
      "scripthost"
    ],
    "listEmittedFiles": false,
    "paths": {
      "datahub-web/*": [
        "app/*"
      ],
      "datahub-web/tests/*": [
        "tests/*"
      ],
      "datahub-web/mirage/*": [
        "mirage/*"
      ],
      "ember-nearley": [
        "lib/ember-nearley/addon"
      ],
      "ember-nearley/*": [
        "lib/ember-nearley/addon/*"
      ],
      "ember-nearley/test-support": [
        "lib/ember-nearley/addon-test-support"
      ],
      "ember-nearley/test-support/*": [
        "lib/ember-nearley/addon-test-support/*"
      ],
      "@datahub/data-models": [
        "../../@datahub/data-models/addon"
      ],
      "@datahub/data-models/*": [
        "../../@datahub/data-models/addon/*"
      ],
      "@datahub/metadata-types": [
        "../../@datahub/metadata-types/addon"
      ],
      "@datahub/metadata-types/*": [
        "../../@datahub/metadata-types/addon/*"
      ],
      "@datahub/entities": [
        "../../@datahub/entities/addon"
      ],
      "@datahub/entities/*": [
        "../../@datahub/entities/addon/*"
      ],
      "@datahub/utils": [
        "../../@datahub/utils/addon"
      ],
      "@datahub/utils/*": [
        "../../@datahub/utils/addon/*"
      ],
      "@datahub/shared": [
        "../../@datahub/shared/addon"
      ],
      "@datahub/shared/*": [
        "../../@datahub/shared/addon/*"
      ],
      "@nacho-ui/core": [
        "../../@nacho-ui/core/addon"
      ],
      "@nacho-ui/core/*": [
        "../../@nacho-ui/core/addon/*"
      ]
    }
  },
  "include": [
    "app/**/*",
    "tests/**/*",
    "mirage/**/*",
    "../../@datahub/shared/types/entity-page/**/*",
    "../../@datahub/metadata-types/addon/**/*",
    "../../@datahub/metadata-types/types/**/*",
    "../../@datahub/utils/addon/**/*",
    "../../@datahub/utils/types/**/*",
    "../../@datahub/shared/addon/**/*",
    "../../@datahub/shared/types/**/*",
    "../../@datahub/shared/tests/**/*",
    "../../@datahub/entities/addon/**/*",
    "../../@datahub/entities/types/**/*",
    "../../@datahub/data-models/addon/**/*",
    "../../@datahub/data-models/types/**/*",
    "../../@datahub/data-models/tests/**/*",
    "../../@nacho-ui/core/addon/**/*",
    "../../@nacho-ui/core/types/**/*"
  ]
}